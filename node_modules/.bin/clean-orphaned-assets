#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/UKK/lobaca/node_modules/.pnpm/laravel-vite-plugin@2.0.1_v_2f21a2b37c44bdd8f1d0cb47b1ef326a/node_modules/laravel-vite-plugin/bin/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/laravel-vite-plugin@2.0.1_v_2f21a2b37c44bdd8f1d0cb47b1ef326a/node_modules/laravel-vite-plugin/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/laravel-vite-plugin@2.0.1_v_2f21a2b37c44bdd8f1d0cb47b1ef326a/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/UKK/lobaca/node_modules/.pnpm/laravel-vite-plugin@2.0.1_v_2f21a2b37c44bdd8f1d0cb47b1ef326a/node_modules/laravel-vite-plugin/bin/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/laravel-vite-plugin@2.0.1_v_2f21a2b37c44bdd8f1d0cb47b1ef326a/node_modules/laravel-vite-plugin/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/laravel-vite-plugin@2.0.1_v_2f21a2b37c44bdd8f1d0cb47b1ef326a/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../laravel-vite-plugin/bin/clean.js" "$@"
else
  exec node  "$basedir/../laravel-vite-plugin/bin/clean.js" "$@"
fi
