#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/UKK/lobaca/node_modules/.pnpm/vite@7.1.11_@types+node@24._77de5c8151682f8b5758780ea294d03a/node_modules/vite/bin/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/vite@7.1.11_@types+node@24._77de5c8151682f8b5758780ea294d03a/node_modules/vite/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/vite@7.1.11_@types+node@24._77de5c8151682f8b5758780ea294d03a/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/UKK/lobaca/node_modules/.pnpm/vite@7.1.11_@types+node@24._77de5c8151682f8b5758780ea294d03a/node_modules/vite/bin/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/vite@7.1.11_@types+node@24._77de5c8151682f8b5758780ea294d03a/node_modules/vite/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/vite@7.1.11_@types+node@24._77de5c8151682f8b5758780ea294d03a/node_modules:/mnt/c/UKK/lobaca/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@7.1.11_@types+node@24._77de5c8151682f8b5758780ea294d03a/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@7.1.11_@types+node@24._77de5c8151682f8b5758780ea294d03a/node_modules/vite/bin/vite.js" "$@"
fi
